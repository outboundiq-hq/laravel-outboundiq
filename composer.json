{
    "name": "outboundiq/laravel-outboundiq",
    "description": "OutboundIQ integration for Laravel - Third-party API monitoring and analytics",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "api",
        "monitoring",
        "tracking",
        "http",
        "observability",
        "metrics"
    ],
    "homepage": "https://outboundiq.dev",
    "authors": [
        {
            "name": "OutboundIQ",
            "email": "hello@outboundiq.dev"
        }
    ],
    "support": {
        "email": "hello@outboundiq.dev",
        "issues": "https://github.com/outboundiq-hq/laravel-outboundiq/issues",
        "source": "https://github.com/outboundiq-hq/laravel-outboundiq"
    },
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/http": "^10.0|^11.0|^12.0",
        "illuminate/contracts": "^10.0|^11.0|^12.0",
        "outboundiq/outboundiq-php": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "OutboundIQ\\Laravel\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "OutboundIQ\\Laravel\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "OutboundIQ\\Laravel\\Providers\\OutboundIQServiceProvider"
            ]
        }
    }
} 