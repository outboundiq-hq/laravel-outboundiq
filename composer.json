{
    "name": "outboundiq/laravel-outboundiq",
    "description": "OutboundIQ integration for Laravel",
    "type": "library",
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^12.0",
        "illuminate/http": "^8.0|^9.0|^10.0|^12.0",
        "illuminate/contracts": "^8.0|^9.0|^10.0|^12.0",
        "outboundiq/php": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "OutboundIQ\\Laravel\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "OutboundIQ\\Laravel\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "OutboundIQ\\Laravel\\Providers\\OutboundIQServiceProvider"
            ]
        }
    }
} 